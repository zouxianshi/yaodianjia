    "@merchant/commons": "^1.0.0-beta.109",
    "area-data": "^5.0.6",
    "axios": "0.18.1",
    "codemirror": "5.45.0",
    "element-ui": "2.11.0",
    "jquery": "^3.4.1",
    "js-cookie": "2.2.0",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.15",
    "normalize.css": "7.0.0",
